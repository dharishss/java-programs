package com.arrays;

import java.util.Arrays;

public class Arrayreverse {

	public static void main(String[] args) {
            int[] arr = {1,2,3,4,5,6,7};
            int start = arr[0];
            int last = arr.length-1;
            for (int i = 0; i < arr.length; i++) {
				int temp = last;
				last = arr[i];
				start = temp;
			}
            System.out.println(Arrays.toString(arr));
            		
            }
	}

}
